{
  "name": "pm-rpc",
  "version": "3.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/pm-rpc.git"
  },
  "scripts": {
    "build": "tsc && webpack --config webpack.dev.js && webpack --config webpack.prod.js && webpack --config webpack.prod.node.js",
    "unit": "cross-env NODE_ENV=test karma start --single-run",
    "lint": "eslint .",
    "test": "npm run lint && npm run unit && npm run integration && npm run test:node",
    "test:node": "node ./integration/node/node-worker-threads.js",
    "prepare": "npm run build",
    "integration": "cross-env NODE_ENV=test karma start integration.karma.conf.js"
  },
  "files": [
    "build",
    "types"
  ],
  "main": "build/pm-rpc.node.min.js",
  "browser": "build/pm-rpc.min.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "cross-env": "5.1.4",
    "eslint": "4.19.0",
    "eslint-config-wix-editor": "2.1.0",
    "eslint-plugin-jasmine": "^4.1.0",
    "eslint-plugin-lodash": "2.7.0",
    "jasmine-core": "3.5.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "3.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "3.0.0",
    "lodash": "4.17.21",
    "typescript": "^4.3.5",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-merge": "4.1.2"
  }
}
